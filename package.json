{
  "name": "simple-express-server",
  "version": "1.0.0",
  "main": "app.js",
  "bin": "app.js",
  "scripts": {
    "start": "node app.js",
    "pkg:win": "pkg . --targets node18-win-x64 --output dist/cody-oai-router.exe",
    "pkg:linux": "pkg . --targets node18-linux-x64 --output dist/cody-oai-router-linux",
    "build:all": "npm run pkg:win && npm run pkg:linux"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  },
  "pkg": {
    "assets": [],
    "targets": ["node18-win-x64", "node18-linux-x64"]
  }
}